<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">

<managed-bean>
	<managed-bean-name>randomNumberController</managed-bean-name>
	<managed-bean-class>com.harsh.ajaxJSFDemo.RandomNumberController</managed-bean-class>
	<managed-bean-scope>request</managed-bean-scope>
</managed-bean>

<managed-bean>
	<managed-bean-name>randomNumberControllerAjax</managed-bean-name>
	<managed-bean-class>com.harsh.ajaxJSFDemo.RandomNumberControllerAjax</managed-bean-class>
	<managed-bean-scope>request</managed-bean-scope>
</managed-bean>

<managed-bean>
	<managed-bean-name>randomNumberControllerAjaxWithExecute</managed-bean-name>
	<managed-bean-class>com.harsh.ajaxJSFDemo.RandomNumberControllerAjaxWithExecute</managed-bean-class>
	<managed-bean-scope>request</managed-bean-scope>
</managed-bean>

<managed-bean>
	<managed-bean-name>stateAndCityController</managed-bean-name>
	<managed-bean-class>com.harsh.ajaxJSFDemo.StateAndCityController</managed-bean-class>
	<managed-bean-scope>session</managed-bean-scope>
</managed-bean>

</faces-config>
